{
  "manifest_version": 2,

  "name": "Detab",
  "description": "Removes extra tabs opened to the same URL.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Remove duplicate tabs"
  },

  "background": {
    "scripts": ["detab.js"]
  },

  "permissions": [ "tabs" ]
}
